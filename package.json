  "name": "fullstack-task-manager-client",
  "repository": "https://github.com/TituxMetal/fullstack-task-manager-client",
  "version": "1.0.0",
  "scripts": {
    "clear": "rm -rf ./.parcel-cache ./.cache ./dist",
    "build": "parcel build ./src/index.html --public-url ./",
    "prebuild": "yarn clear",
    "watch": "env-cmd -f ./.env parcel src/index.html -p 3000"
  },
  "dependencies": {
    "@fontsource/roboto": "4.5.7",
    "@hookform/resolvers": "2.8.10",
    "@styled-icons/material": "10.34.0",
    "axios": "0.27.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.31.1",
    "react-query": "3.39.0",
    "react-router-dom": "6.3.0",
    "styled-components": "5.3.5",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.18.0",
    "@babel/plugin-transform-react-jsx": "7.17.12",
    "autoprefixer": "10.4.7",
    "env-cmd": "10.1.0",
    "eslint": "8.15.0",
    "eslint-config-kentcdodds": "19.2.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-alias": "1.1.2",
    "parcel-bundler": "1.12.5",
    "postcss": "8.4.14",
    "prettier": "2.6.2",
    "tailwindcss": "3.0.24",
    "twin.macro": "2.8.2"
  },
  "alias": {
    "~": "./src"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "browserslist": [
    ">10%",
    "chrome 76",
    "not dead",
    "not op_mini all"
  ]
}
