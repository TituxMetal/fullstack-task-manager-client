  "name": "fullstack-task-manager-client",
  "repository": "https://github.com/TituxMetal/fullstack-task-manager-client",
  "version": "1.0.0",
  "scripts": {
    "clear": "rm -rf ./.parcel-cache ./.cache ./dist",
    "build": "parcel build ./src/index.html --public-url ./",
    "prebuild": "yarn clear",
    "watch": "env-cmd -f ./.env parcel src/index.html -p 3000"
  },
  "dependencies": {
    "@fontsource/roboto": "4.5.8",
    "@hookform/resolvers": "2.9.10",
    "@styled-icons/material": "10.46.0",
    "axios": "1.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.40.0",
    "react-query": "3.39.2",
    "react-router-dom": "6.5.0",
    "styled-components": "5.3.6",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/plugin-transform-react-jsx": "7.19.0",
    "autoprefixer": "10.4.13",
    "env-cmd": "10.1.0",
    "eslint": "8.29.0",
    "eslint-config-kentcdodds": "19.2.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-alias": "1.1.2",
    "parcel-bundler": "1.12.5",
    "postcss": "8.4.20",
    "prettier": "2.8.1",
    "tailwindcss": "3.2.4",
    "twin.macro": "3.1.0"
  },
  "alias": {
    "~": "./src"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "browserslist": [
    ">10%",
    "chrome 76",
    "not dead",
    "not op_mini all"
  ]
}
