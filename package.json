    "watch": "env-cmd -f ./.env.development parcel src/index.html -p 3000"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.3.0",
    "@styled-icons/material": "^10.28.0",
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint": "^7.27.0",
    "eslint-config-kentcdodds": "^17.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "parcel": "^2.0.0-beta.2",
    "prettier": "^2.3.0"
  },
  "alias": {
    "#root": "./src"
  },
  "browserslist": [
    ">10%",
    "chrome 76",
    "not dead",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TituxMetal/fullstack-task-manager-client"
  }
